{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/src/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/socket.io/(.*)",
        "dest": "index..js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }